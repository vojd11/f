<odoo>
  <!-- <data> -->
  <record id="slide_qgen_view_form" model="ir.ui.view">
    <field name="name">slide.qgen.view.form</field>
    <field name="model">slide.qgen</field>
    <field name="arch" type="xml">
      <form string="Generate quetions">
        <sheet>
          <group>
            <field name="name"/>
            <field name="channel_name" require="true"/>
            <field name="origin"/>
            <field name="result"/>
          </group>
          <button name="gen_quetions" type="object" class="btn-primary" icon="fa-magic" string='Generate'></button>
        </sheet>
        <notebook>
          <page name="quetions" string="quetions">
            <field name="question_ids" nolabel="1">
              <tree>
                <field name="sequence" widget="handle"/>
                <field name="question"/>
              </tree>
            </field>
          </page>
        </notebook>
      </form>
    </field>
  </record>

  <!-- <record id="slide_qgen_action_window" model="ir.actions.act_window.view"> <field name="sequence" eval="1"/> <field name="view_mode">form</field> <field name="act_window_id" ref="slide_qgen_view_form"/> <field name="view_id" ref="slide_qgen_view_form"/> </record> -->
